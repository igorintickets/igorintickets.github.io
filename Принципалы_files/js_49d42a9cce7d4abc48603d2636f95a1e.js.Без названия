(function($){'use strict';$(function(){$("#table-search-input").keyup(function(event){var $rows=$("table.searchable tbody tr");if(27===event.keyCode)$(this).val(""),$rows.show();else if(""===$(this).val())$rows.show();else{$rows.show();for(var search_arr=$(this).val().trim();search_arr.match("  ");)search_arr=search_arr.replace("  "," ");search_arr=search_arr.split(" ");for(var _loop=function(i){var str=search_arr[i];$rows.each(function(){0>$(this).children("td").text().search(new RegExp(str,"i"))&&$(this).hide()})},i=0;i<search_arr.length;i++)_loop(i)}})})})(jQuery);;
